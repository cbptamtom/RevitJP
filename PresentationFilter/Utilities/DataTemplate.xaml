<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:PresentationFilter.Views"
    xmlns:vm="clr-namespace:PresentationFilter.ViewModels">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:Home />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FilterManagerViewModel}">
        <view:FilterManager />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DeleteFilterViewModel}">
        <view:DeleteFilter />
    </DataTemplate>




</ResourceDictionary>